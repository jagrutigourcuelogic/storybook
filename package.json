{
  "name": "@jagrutigourcuelogic/storybook_pract5",
  "version": "0.1.0",
  "main": "dist/index.js",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "chromatic": "npx chromatic --project-token 7a8211f64a9e",
    "build": "cross-env BABEL_ENV=production babel src -d dist"
  },
  "repository": "https://github.com/jagrutigourcuelogic/storybook.git",
  "keywords": [
    "npm",
    "npm",
    "practice"
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "Jagruti",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jagrutigourcuelogic/storybook/issues"
  },
  "homepage": "https://github.com/jagrutigourcuelogic/storybook#readme",
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "auto": "^10.29.2",
    "cross-env": "^7.0.3"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  },
  "description": "storybook design practice",
  "private": false
}
